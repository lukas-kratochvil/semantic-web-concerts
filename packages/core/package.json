{
  "name": "@music-event-connect/core",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "prebuild": "rm -rf dist *.tsbuildinfo",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint --fix \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "tidy": "pnpm format && pnpm lint"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./entities": {
      "types": "./dist/entities/index.d.ts",
      "require": "./dist/entities/index.js",
      "default": "./dist/entities/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "require": "./dist/interfaces/index.js",
      "default": "./dist/interfaces/index.js"
    },
    "./rdf": {
      "types": "./dist/rdf/index.d.ts",
      "require": "./dist/rdf/index.js",
      "default": "./dist/rdf/index.js"
    }
  },
  "devDependencies": {
    "@music-event-connect/eslint-config": "workspace:*",
    "@types/js-yaml": "^4.0.9",
    "@types/n3": "^1.26.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@music-event-connect/shared": "workspace:*",
    "@nestjs/common": "^11.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "n3": "^1.26.0",
    "nest-winston": "^1.10.2",
    "rdf-namespaces": "^1.16.0",
    "reflect-metadata": "^0.2.2",
    "uuidv7": "^1.0.2",
    "winston": "^3.17.0"
  }
}
